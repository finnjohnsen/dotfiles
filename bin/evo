#!/usr/bin/python3

import requests
url="https://visits.evofitness.no/api/v1/locations/0b3390cc-a035-4262-a188-12b2a829d455/current"

RED = "\033[31m"
GREEN = "\033[32m"
BLUE = "\033[34m"
BOLD = '\033[1m'

RESET = "\033[0m"

response = requests.get(url)
json = response.json()

print(f"{BOLD}{json['current']}{RESET} trener nå på {json['name']} (maksimalt {json['max_capacity']})")
